{
  "id": "stribe-merch",
  "name": "STRIBE Merch",
  "version": "1.0.0",
  "main": "index.html",
  "host": [{ "app": "PS", "minVersion": "25.12.0" }],
  "manifestVersion": 5,
  "featureFlags": { "enableAlerts": true },
  "entrypoints": [{
    "type": "panel",
    "id": "main",
    "minimumSize": { "width": 230, "height": 200 },
    "maximumSize": { "width": 2000, "height": 2000 },
    "preferredDockedSize": { "width": 230, "height": 450 },
    "preferredFloatingSize": { "width": 230, "height": 450 },
    "icons": [
      { "width": 32, "height": 32, "path": "icons/icon_D.png", "scale": [1, 2], "theme": ["dark", "darkest"], "species": ["generic"] },
      { "width": 32, "height": 32, "path": "icons/icon_N.png", "scale": [1, 2], "theme": ["lightest", "light"], "species": ["generic"] }
    ],
    "label": { "default": "Merch" }
  }],
  "icons": [
    { "width": 23, "height": 23, "path": "icons/dark.png", "scale": [1, 2], "theme": ["darkest", "dark", "medium"] },
    { "width": 23, "height": 23, "path": "icons/light.png", "scale": [1, 2], "theme": ["lightest", "light"] }
  ],
  "requiredPermissions": {
    "network": {
      "domains": [
        "stribe-api.vercel.app",
        "https://docs.google.com",
        "pub-3c06366d547445298c77e04b7c3c77ad.r2.dev"
      ]
    }
  }
}